{
  "name": "grapesjs-open-graph-embed-card",
  "description": "open-graph-embed-card",
  "main": "dist/grapesjs-open-graph-embed-card.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/booellean/grapesjs-open-graph-embed-card.git"
  },
  "scripts": {
    "start": "grapesjs-cli serve --host=0.0.0.0",
    "start:backup": "grapesjs-cli serve --host=0.0.0.0 --devServer='{\"disableHostCheck\": true, \"compress\": true, \"inline\": true, \"port\": '80', \"allowedHosts\": [ \".glitch.me\" ]}'",
    "build": "grapesjs-cli build",
    "bump": "npm version patch -m 'Bump v%s'",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "keywords": [
    "grapes",
    "grapesjs",
    "plugin",
    "open graph",
    "open graph protocol",
    "embed",
    "protocol",
    "metadata",
    "data",
    "json",
    "facebook",
    "twitter",
    "twitter cards",
    "twitter card",
    "cards",
    "card",
    "javascript",
    "preview link",
    "hyperlink"
  ],
  "author": "Elliot \"booellean\" Forst",
  "bugs": {
    "url": "https://github.com/booellean/grapesjs-open-graph-embed-card/issues"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@types/jsdom": "^16.2.14",
    "babel-jest": "^27.5.1",
    "grapesjs-cli": "^2.0.0",
    "grapesjs": "^0.17.27",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/tests/specs/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "license": "MIT"
}
